create_table("service_calls") {
	t.Column("id", "uuid", {primary: true})
	t.Column("name", "string", {})
	t.Column("date", "date", {})
	t.Column("start_time", "timestamp", {})
	t.Column("stop_time", "timestamp", {})
	t.Column("reason", "string", {})
	t.Column("work_performed", "string", {})
	t.Timestamps()
}